
{
  "name": "Eclipse",
  "type": "dark",
  "colors": {
    // Base Colors
    "focusBorder": "#9a86fd",
    "foreground": "#e2e2e8",
    "widget.shadow": "#00000000",
    "selection.background": "#2d2b40",
    "errorForeground": "#ff6d85",
    "button.background": "#2b2640",
    "button.foreground": "#e2e2e8",
    "button.hoverBackground": "#3a3550",
    
    // Editor Colors
    "editor.background": "#0f0e15",
    "editor.foreground": "#e2e2e8",
    "editor.lineHighlightBackground": "#161421",
    "editorLineNumber.foreground": "#4a4860",
    "editorLineNumber.activeForeground": "#9a86fd",
    "editor.selectionBackground": "#3b384f",
    "editor.inactiveSelectionBackground": "#2a273f80",
    "editor.wordHighlightBackground": "#7aa2f730",
    "editor.findMatchBackground": "#7aa2f760",
    "editor.findMatchHighlightBackground": "#7aa2f740",
    "editorBracketMatch.background": "#3b384f",
    "editorBracketMatch.border": "#9a86fd",
    
    // Sidebar
    "sideBar.background": "#0d0c16",
    "sideBar.foreground": "#c8c8e0",
    "sideBarSectionHeader.background": "#141222",
    "sideBarTitle.foreground": "#f0f0f8",
    "sideBarSectionHeader.foreground": "#e2e2e8",
    
    // Status Bar
    "statusBar.background": "#0d0c16",
    "statusBar.foreground": "#c8c8e0",
    "statusBar.noFolderBackground": "#0d0c16",
    "statusBar.debuggingBackground": "#7a5af5",
    "statusBar.debuggingForeground": "#ffffff",
    "statusBarItem.activeBackground": "#1e1c2e",
    "statusBarItem.hoverBackground": "#2a273f",
    "statusBarItem.prominentBackground": "#1a1825",
    
    // Activity Bar
    "activityBar.background": "#0a0910",
    "activityBar.foreground": "#e2e2e8",
    "activityBarBadge.background": "#9a86fd",
    "activityBarBadge.foreground": "#0a0910",
    
    // Title Bar
    "titleBar.activeBackground": "#0a0910",
    "titleBar.activeForeground": "#e2e2e8",
    "titleBar.inactiveBackground": "#0a0910",
    "titleBar.inactiveForeground": "#6d6b8a",
    
    // Tabs
    "tab.activeBackground": "#0f0e15",
    "tab.inactiveBackground": "#0a0910",
    "tab.activeForeground": "#e2e2e8",
    "tab.inactiveForeground": "#6d6b8a",
    "tab.border": "#1a1825",
    "tab.activeBorder": "#9a86fd",
    "tab.activeBorderTop": "#9a86fd",
    
    // Inputs & Dropdowns
    "input.background": "#161421",
    "input.foreground": "#e2e2e8",
    "input.border": "#252337",
    "input.placeholderForeground": "#6d6b8a",
    "inputOption.activeBorder": "#9a86fd",
    "dropdown.background": "#161421",
    "dropdown.foreground": "#e2e2e8",
    "dropdown.border": "#252337",
    
    // Lists & Trees
    "list.activeSelectionBackground": "#2d2b40",
    "list.activeSelectionForeground": "#e2e2e8",
    "list.hoverBackground": "#252337",
    "list.highlightForeground": "#9a86fd",
    "list.inactiveSelectionBackground": "#1e1c2e",
    "list.focusBackground": "#2d2b40",
    "list.errorForeground": "#ff6d85",
    "list.warningForeground": "#ff9e64",
    
    // Scrollbars
    "scrollbar.shadow": "#00000060",
    "scrollbarSlider.background": "#2d2b4066",
    "scrollbarSlider.hoverBackground": "#3b3a4a99",
    "scrollbarSlider.activeBackground": "#4b486699",
    
    // Panel
    "panel.background": "#0f0e15",
    "panel.border": "#1a1825",
    "panelTitle.activeForeground": "#e2e2e8",
    "panelTitle.inactiveForeground": "#6d6b8a",
    
    // Terminal
    "terminal.background": "#0f0e15",
    "terminal.foreground": "#e2e2e8",
    "terminal.ansiBlack": "#0f0e15",
    "terminal.ansiRed": "#ff6d85",
    "terminal.ansiGreen": "#7efdd0",
    "terminal.ansiYellow": "#ff9e64",
    "terminal.ansiBlue": "#7aa2f7",
    "terminal.ansiMagenta": "#bb9af7",
    "terminal.ansiCyan": "#7dcfff",
    "terminal.ansiWhite": "#e2e2e8",
    "terminal.ansiBrightBlack": "#6d6b8a",
    "terminal.ansiBrightRed": "#ff8b9f",
    "terminal.ansiBrightGreen": "#9effe0",
    "terminal.ansiBrightYellow": "#ffb88f",
    "terminal.ansiBrightBlue": "#a5b8ff",
    "terminal.ansiBrightMagenta": "#d2bfff",
    "terminal.ansiBrightCyan": "#a5dfff",
    "terminal.ansiBrightWhite": "#ffffff"
  },
  "tokenColors": [
    // Comments & Documentation
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#6d6b8a"
      }
    },
    
    // Strings
    {
      "name": "Strings",
      "scope": ["string", "string.quoted", "string.template", "string.interpolated"],
      "settings": {
        "foreground": "#e2c792"
      }
    },
    
    // Keywords
    {
      "name": "Keywords",
      "scope": [
        "keyword", "keyword.control", "storage", "storage.type",
        "variable.language.this", "variable.language.self"
      ],
      "settings": {
        "foreground": "#bb9af7"
      }
    },
    
    // Functions & Methods
    {
      "name": "Functions",
      "scope": [
        "entity.name.function", "support.function", "variable.function",
        "meta.function-call", "support.function.builtin"
      ],
      "settings": {
        "foreground": "#7aa2f7"
      }
    },
    
    // Classes & Types
    {
      "name": "Classes & Types",
      "scope": [
        "entity.name.type", "entity.name.class", "support.class",
        "support.type", "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#7efdd0"
      }
    },
    
    // Variables & Properties
    {
      "name": "Variables",
      "scope": ["variable", "variable.other", "variable.parameter"],
      "settings": {
        "foreground": "#e2e2e8"
      }
    },
    
    // Numbers & Constants
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#ff9e64"
      }
    },
    
    // Operators & Punctuation
    {
      "name": "Operators",
      "scope": ["keyword.operator", "punctuation"],
      "settings": {
        "foreground": "#bb9af7"
      }
    },
    
    // HTML/XML
    {
      "name": "HTML/XML Tags",
      "scope": [
        "entity.name.tag", "meta.tag", "tag.decorator",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#ff6d85"
      }
    },
    {
      "name": "HTML/XML Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#7efdd0"
      }
    },
    
    // CSS
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css", "meta.property-name"],
      "settings": {
        "foreground": "#7aa2f7"
      }
    },
    {
      "name": "CSS Values",
      "scope": ["support.constant.property-value"],
      "settings": {
        "foreground": "#7efdd0"
      }
    },
    
    // JSON
    {
      "name": "JSON Keys",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#7aa2f7"
      }
    },
    
    // Markdown
    {
      "name": "Markdown Headers",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#9a86fd"
      }
    },
    {
      "name": "Markdown Links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#7aa2f7"
      }
    },
    
    // Diff
    {
      "name": "Diff Added",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#7efdd0"
      }
    },
    {
      "name": "Diff Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#ff6d85"
      }
    },
    {
      "name": "Diff Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#ff9e64"
      }
    },
    
    // Special
    {
      "name": "Special Variables",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#bb9af7"
      }
    },
    {
      "name": "Deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#ff6d85",
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#ff6d85"
      }
    }
  ]
}
